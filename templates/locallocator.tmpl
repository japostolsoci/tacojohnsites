								function formatTime(time) {
									if(time == 0000) {
											theTime = "Midnight";
											time = theTime;
									}
									if (time == 9999) {
											theTime = "Midnight";
											time = theTime;
									}
									if (time <= 1200) {
							
											temptime = time.toString();
							
											if(temptime.length == 3) {
												var theTime = time.toString().substring(0, 1) + ":" + time.toString().substring(1, 3) + " AM";
											} else {
												var theTime = time.toString().substring(0, 2) + ":" + time.toString().substring(2, 4) + " AM";
											}
							
											if (theTime[0] == 0) {
													theTime = theTime.substring(1);
											}
							
											if (time == 0000) {
													theTime = "Midnight";
											}
							
											if (time == 1200) {
													theTime = "12:00 PM";
											}

											return theTime;
									} else if (time == 'Closed') {
											return "Closed";
									}
									else if (time == 'Midnight') {
											return "Midnight";
									} else {
											return parseInt(time.toString().substring(0, 2) - 12) + ":" + time.toString().substring(2, 4) + " PM";
									}
							
							}
            });

            function generate_directions (startaddress, endaddress) {

                endaddress = endaddress.replace("#", "poundsign");

                if (bubbleValidate(startaddress)) {
                    $('#directions_url').data('url', '' + startaddress + '&addressline=' + '<TMPL_VAR NAME=latitude>,<TMPL_VAR NAME=longitude>' + '&addresslineDisplay=' + endaddress);
