									<script>
										function isAfterToday(dateString) {
											const inputDate = dateString.split('-');
											const today = new Date();

											const inputYear = parseInt(inputDate[0], 10);
											const inputMonth = parseInt(inputDate[1], 10) - 1;
											const inputDay = parseInt(inputDate[2], 10);

											const currentYear = today.getFullYear();
											const currentMonth = today.getMonth();
											const currentDay = today.getDate();

											if (inputYear > currentYear) {
												return true;
											} else if (inputYear === currentYear) {
												if (inputMonth > currentMonth) {
													return true;
												} else if (inputMonth === currentMonth) {
													return inputDay > currentDay;
												}
											}
											return false;
										}

										if (isAfterToday('<tmpl_var reopen_date>')) {
											document.querySelector('#reopened_date_dom').style.display = ''
										}
									</script>
